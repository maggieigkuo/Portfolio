x="1" value="57"/><column index="2" value="300"/><column index="3" value="64"/><column index="4" value="92"/><column index="5" value="61"/><column index="6" value="300"/><column index="7" value="89"/><column index="8" value="62"/><column index="9" value="68"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">CREATE VIEW Q1 AS
SELECT InvoiceId,  InvoiceDate,  Total
FROM invoices
ORDER BY InvoiceDate DESC
LIMIT 10;

CREATE VIEW Q2 AS
SELECT FirstName, LastName, Address, City, State, PostalCode
FROM customers
WHERE Country = &quot;USA&quot;; 

CREATE VIEW Q3 AS
SELECT Name, Composer
FROM tracks
WHERE Composer = &quot;Samuel Rosa&quot;; 

CREATE VIEW Q4 AS
SELECT DISTINCT Country
FROM customers;

CREATE VIEW Q5 AS
SELECT Name, 
round(Milliseconds/60000, 1) AS minutes, 
round((Bytes/1024), 0) AS kilobytes
FROM tracks;

CREATE VIEW Q6 AS
SELECT count(*)
FROM tracks
WHERE Name like '%(Live)%'; 

CREATE VIEW Q7 AS
SELECT FirstName, LastName, 
coalesce(Fax, Phone) as Contact
FROM customers;

CREATE VIEW Q8 AS
SELECT EmployeeId, 
FirstName, LastName, 
substr(timediff(HireDate, BirthDate), 4, 2) as HireAge,
Email
FROM employees;

CREATE VIEW Q9 AS
SELECT BillingCountry, count(*) as num_orders,
round(avg(total), 2)as ave_cost
FROM invoices
GROUP BY 	BillingCountry; 

CREATE VIEW Q10 AS
SELECT 
   COUNT(*) AS &quot;Total&quot;,
   round(100.0*SUM(Name LIKE '%Feat.%')/count(*), 2) AS &quot;Percent_Feat.&quot;,
   round(100.0 - (100.0*SUM(Name LIKE '%Feat.%') / COUNT(*)), 2) AS &quot;Percent_Individual_Groups&quot;
FROM artists;

CREATE VIEW Q11 AS
SELECT 
    SUBSTR(Name, 1, 1) AS First_Letter,
    COUNT(*) AS Artist_Count
FROM artists
WHERE First_Letter BETWEEN 'A' AND 'Z'
GROUP BY First_Letter
ORDER BY First_Letter;

CREATE VIEW Q12 AS
SELECT 
    tracks.Name AS SongName,
    media_types.Name AS MediaType
FROM tracks
JOIN media_types ON tracks.MediaTypeId = media_types.MediaTypeId;

CREATE VIEW Q13 AS
SELECT 
	genres.Name AS Genre,
	count(tracks.GenreId) as Num_Tracks
FROM genres
JOIN tracks ON genres.GenreId = tracks.GenreId
GROUP BY genres.Name
ORDER BY num_tracks DESC;

CREATE VIEW Q14 AS
SELECT
	tracks.Name as Songs,
	albums.Title as Album,
	artists.Name as Artist
FROM tracks
JOIN albums ON tracks.AlbumId = albums.AlbumId
JOIN artists ON albums.ArtistId = artists.ArtistId; 

CREATE VIEW Q15 AS
SELECT
	customers.FirstName as First,
	customers.LastName as Last,
	customers.Address as Address,
	SUM(invoices.Total) as Spent
FROM customers
JOIN invoices ON customers.CustomerId = invoices.CustomerId
GROUP BY customers.CustomerId;








